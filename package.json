{
  "name": "webpack-ts",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "gulp",
    "supremacy": "stylus-supremacy format ./src/**/*.styl -p ./stylus-supremacy.json -r",
    "tslint": "tslint --fix src/**/*.ts",
    "stylint": "stylint src/assets/stylus",
    "build:develop": "rm -rf dist/ && gulp build",
    "build": "rm -rf dist/ && gulp build --production",
    "deploy": "gulp build --production && mv ._headers dist/_headers",
    "commit:stylint": "yarn run supremacy && yarn run stylint",
    "commit:tslint": "yarn run tslint",
    "codecrumbs": "codecrumbs -d src/assets/js -e src/assets/js/common.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.styl": [
      "yarn run commit:stylint",
      "git add"
    ],
    "src/*.ts": [
      "yarn run commit:tslint",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "codecrumbs": "^1.7.3",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-data": "^1.3.1",
    "gulp-filter": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-mode": "^1.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylus": "^2.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "postcss-cssnext": "^3.1.0",
    "prettier": "^1.18.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "style-loader": "^1.0.0",
    "stylint": "^2.0.0",
    "stylint-stylish": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.12.7",
    "ts-loader": "^6.1.2",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tsutils": "^3.17.1",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-stream": "^5.2.1",
    "yarn": "^1.17.3"
  },
  "dependencies": {
    "viewport-extra": "^1.0.4"
  }
}
