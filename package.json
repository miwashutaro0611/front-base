{
  "name": "webpack-ts",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "gulp",
    "precommit": "lint-staged",
    "supremacy": "stylus-supremacy format ./src/**/*.styl -p ./stylus-supremacy.json -r",
    "eslint": "eslint --fix src",
    "build:develop": "rm -rf dist/ && gulp build",
    "build:production": "rm -rf dist/ && gulp build --production"
  },
  "lint-staged": {
    "*.styl": [
      "stylus-supremacy format ./src/**/*.styl -p ./stylus-supremacy.json -r",
      "git add"
    ],
    "*.js": [
      "eslint --fix src",
      "git add"
    ]
  },
