{
  "name": "front-base",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.common.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "supremacy": "stylus-supremacy format ./src/**/*.styl -p ./stylus-supremacy.json -r",
    "eslint": "eslint --fix --ext .js,.ts --ignore-path .gitignore .",
    "stylint": "stylint src/assets/stylus",
    "lint": "yarn stylint && yarn eslint",
    "build:develop": "rm -rf dist/ && webpack --config webpack.dev.js",
    "build": "rm -rf dist/ && webpack --config webpack.prod.js",
    "deploy": "webpack --config webpack.prod.js && mv ._headers dist/_headers",
    "deploy:master": "webpack --config webpack.prod.js",
    "commit:stylint": "yarn supremacy && yarn stylint",
    "commit:eslint": "yarn eslint",
    "codecrumbs": "codecrumbs -d src/assets/js -e src/assets/js/common.ts",
    "jest": "jest",
    "jest-coverage": "yarn jest -- --coverage",
    "pug-prettier": "prettier --write '**/*.pug'"
  },
  "lint-staged": {
    "*.pug": [
      "yarn pug-prettier"
    ],
    "*.styl": [
      "yarn commit:stylint"
    ],
    "*.ts": [
      "yarn commit:eslint"
    ],
    "*.spec.ts": [
      "yarn jest"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@prettier/plugin-pug": "^1.4.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.1.0",
    "codecrumbs": "^1.7.3",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "style-loader": "^1.2.1",
    "stylint": "^2.0.0",
    "stylint-stylish": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.5",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "typescript-require": "^0.2.10",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "viewport-extra": "^1.0.4"
  }
}
