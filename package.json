{
  "name": "front-base",
  "engines": {
    "npm": "please_use_yarn_instead",
    "node": "16.13.1"
  },
  "private": true,
  "scripts": {
    "build": "yarn script:webp && rm -rf dist/ && webpack --config webpack.prod.js",
    "build:develop": "yarn script:webp && rm -rf dist/ && webpack --config webpack.dev.js",
    "deploy": "yarn webp && webpack --config webpack.prod.js && mv ._headers dist/_headers",
    "deploy:master": "yarn webp && webpack --config webpack.prod.js",
    "dev": "yarn script:webp && webpack-dev-server --config webpack.dev.js",
    "lint": "run-p lint:*",
    "lint:js": "eslint --fix --ext .js,.ts --ignore-path .gitignore .",
    "lint:package": "fixpack",
    "lint:pug": "prettier --write '**/*.pug'",
    "lint:style": "stylelint --fix '**/*.{scss,css}' --ignore-path .gitignore",
    "script": "run-p script:*",
    "script:webp": "rm -rf ./src/assets/img-webp && mkdir ./src/assets/img-webp && node ./script/create-webp.js",
    "test": "jest",
    "test:ci": "jest -i --ci --runInBand"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@barba/css": "^2.1.15",
    "@barba/prefetch": "^2.1.10",
    "autoprefixer": "^10.4.1",
    "luxy.js": "^0.1.0",
    "picturefill": "^3.0.3",
    "ua-parser-js": "^0.7.28",
    "viewport-extra": "^2.1.1",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@prettier/plugin-pug": "^1.16.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fixpack": "^4.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "7.0.1",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.3.2",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.37.5",
    "sass-loader": "10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-no-unsupported-browser-features": "^5.0.1",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.3.5",
    "typescript-require": "^0.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "yarn": "^1.22.11"
  },
  "lint-staged": {
    "*.pug": "yarn lint:pug",
    "*.scss": "yarn lint:style",
    "*.ts": "yarn lint:js",
    "*.spec.ts": "yarn test",
    "package.json": "yarn lint:package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "UNLICENSED"
}
