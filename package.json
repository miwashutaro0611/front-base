{
  "name": "front-base",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.common.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "supremacy": "stylus-supremacy format ./src/**/*.styl -p ./stylus-supremacy.json -r",
    "eslint": "eslint --fix src/**/*.ts",
    "stylint": "stylint src/assets/stylus",
    "build:develop": "rm -rf dist/ && webpack --config webpack.dev.js",
    "build": "rm -rf dist/ && webpack --config webpack.prod.js",
    "deploy": "webpack --config webpack.prod.js && mv ._headers dist/_headers",
    "deploy:master": "webpack --config webpack.prod.js",
    "commit:stylint": "yarn supremacy && yarn stylint",
    "commit:eslint": "yarn eslint",
    "codecrumbs": "codecrumbs -d src/assets/js -e src/assets/js/common.ts"
  },
  "lint-staged": {
    "*.styl": [
      "yarn commit:stylint",
      "git add"
    ],
    "*.ts": [
      "yarn commit:eslint",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "codecrumbs": "^1.7.3",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-data": "^1.3.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-mode": "^1.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylus": "^2.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "4.2.1",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "style-loader": "^1.2.1",
    "stylint": "^2.0.0",
    "stylint-stylish": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "typescript-require": "^0.2.10",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-stream": "^5.2.1",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "viewport-extra": "^1.0.4"
  }
}
