{
  "name": "front-base",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.common.js",
  "scripts": {
    "start": "gulp",
    "supremacy": "stylus-supremacy format ./src/**/*.styl -p ./stylus-supremacy.json -r",
    "eslint": "eslint --fix src/**/*.ts",
    "stylint": "stylint src/assets/stylus",
    "build:develop": "rm -rf dist/ && gulp build",
    "build": "rm -rf dist/ && gulp build --production",
    "deploy": "gulp build --production && mv ._headers dist/_headers",
    "deploy:master": "gulp build --production",
    "commit:stylint": "yarn supremacy && yarn stylint",
    "commit:eslint": "yarn eslint",
    "codecrumbs": "codecrumbs -d src/assets/js -e src/assets/js/common.ts"
  },
  "lint-staged": {
    "*.styl": [
      "yarn commit:stylint",
      "git add"
    ],
    "*.ts": [
      "yarn commit:eslint",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@types/node": "^12.12.31",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "codecrumbs": "^1.7.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-data": "^1.3.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-mode": "^1.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-pug-linter": "^1.3.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylus": "^2.7.0",
    "husky": "4.2.3",
    "lint-staged": "^10.0.9",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "pug": "^2.0.4",
    "pug-lint": "^2.6.0",
    "stylint": "^2.0.0",
    "stylint-stylish": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-supremacy": "^2.14.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "typescript-require": "^0.2.10",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-stream": "^5.2.1",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "autoprefixer": "^9.7.5",
    "viewport-extra": "^1.0.4"
  }
}
